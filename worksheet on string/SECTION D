// 6. Check whether the string "developer" contains the word "love"
console.log('"developer" contains "love":', "developer".includes("love")); // false

// 7. Verify whether the string "AlphaIntern" begins with "Alpha"
console.log('"AlphaIntern" begins with "Alpha":', "AlphaIntern".startsWith("Alpha")); // true

// 8. Check if the string "AlphaIntern" ends with "tern"
console.log('"AlphaIntern" ends with "tern":', "AlphaIntern".endsWith("tern")); // true

// 9. Find the position of the first occurrence of the character 'a' in "JavaScript"
console.log('Position of "a" in "JavaScript":', "JavaScript".indexOf("a")); // 1

// 10. Check if the word "Backend" is present inside "I am a Backend Developer"
console.log('"I am a Backend Developer" contains "Backend":', "I am a Backend Developer".includes("Backend")); // true
